<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Proyecto1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Proyecto1"
    xmlns:pages="clr-namespace:Proyecto1.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="Proyecto1">

    <ShellContent
        Title="Flights"
        ContentTemplate="{DataTemplate pages:FlightsPage}"
        Route="FlightPage" />
    <ShellContent
        Title="Flights Connections"
        ContentTemplate="{DataTemplate pages:FlightConnectionsPage}"
        Route="FlightConnectionsPage" />

</Shell>
